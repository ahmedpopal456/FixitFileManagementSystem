{
  "KeyVault": {
    "KeyVaultUri": "{{keyVaultUri}}",
    "ClientId": "{{keyvaultServicePrincipalId}}",
    "ClientSecret": "{{keyvaultServicePrincipalSecret}}"
  },
  "ApplicationInsights": {
    "InstrumentationKey": "{{appInsightsKey}}"
  },
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Warning",
      "System": "Information",
      "Microsoft": "Information"
    },
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "ASPNETCORE_DETAILEDERRORS": true,
  "FIXIT-SA-AN": "{{empowerStorageName}}",
  "FIXIT-SA-EP": "{{empowerStorageEndpoint}}",
  "FMSEventGrid": {
    "FIXIT-FMS-EG-ONIMAGEEXPIRED-TE": "{{empowerEventGridOnImageUrlExpiredTopicEndpoint}}",
    "FIXIT-FMS-EG-ONIMAGEEXPIRED-TK": "{{empowerEventGridOnImageUrlExpiredTopicKey}}",
    "FIXIT-FMS-EG-ONIMAGEURLSUPDATE-TE": "{{empowerEventGridOnImageUrlsUpdateTopicEndpoint}}",
    "FIXIT-FMS-EG-ONIMAGEURLSUPDATE-TK": "{{empowerEventGridOnImageUrlsUpdateTopicKey}}"
  },
  "FIXIT-FMS-THUMBNAILS-CONTAINER": "thumbnails",
  "FIXIT-FMS-ASSETS-CONTAINER": "assets",
  "FIXIT-FMS-DOWNLOADLINK-EXPIRYTIME": "4",
  "FIXIT-FMS-THUMBNAILLINK-EXPIRYTIME": "876000",
  "FIXIT-FMS-BASELINK-EXPIRYTIME": "876000",
  "FIXIT-FMS-CONFIGURATION-TABLE": "fileSystemConfigurationTable",
  "FIXIT-FMS-CONFIGURATION-SETTINGS": "fileSystemConfigurationSettings"
}
